{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@cappasoft/openrouter-model-selector": ["../../packages/openrouter-model-selector/src/index.ts"],
      "@cappasoft/openrouter-model-selector/styles.css": ["../../packages/openrouter-model-selector/dist/styles.css"],
      "@cappasoft/openrouter-models": ["../../packages/openrouter-models/src/index.ts"]
    }
  },
  "include": ["src", "vite.config.ts"]
}


